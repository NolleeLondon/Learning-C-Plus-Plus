#include <iostream>
using namespace std;
int main() {
    int arr[6] = {1, 2, 3, 4, 5, 6};
    int before = 0;
    int after = 0;
    
    for(int i = 0; i < 6; i++){
        before = arr[i];
        cout << before << endl;
    }
    
    cout << "REVERSE: " << endl;
    
    
    for(int i = 5; i > -1; i--){
        after = arr[i];
        cout << arr[i] << endl;
    }
    
}
