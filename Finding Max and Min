#include <iostream>
using namespace std;
int main() {
    int SIZE = 7;
    int numbers[SIZE] = {4, 6, 20, 7, 9, -2, 3};
    
    int minimum = numbers[0];
    int maximum = numbers[0];
    
    for(int i = 0; i < SIZE; i++){
        if(minimum > numbers[i]){
          minimum = numbers[i];  
        } if(maximum < numbers[i]){
            maximum = numbers[i];
        }
    }
    cout << minimum << endl;
    cout << maximum << endl;
}
